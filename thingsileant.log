>>sudo -i
>>pip install pdfkit
>>apt-get install wkhtmltopdf
>>apt-get install xvfb
>>echo -e '#!/bin/bash\nxvfb-run -a --server-args="-screen 0, 1024x768x24" /usr/bin/wkhtmltopdf -q $*' > /usr/bin/wkhtmltopdf.sh
